  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "debug": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "lint": "eslint src/*.ts",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:reset": "db-migrate reset",
    "fix": "eslint --fix src/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "npm run migrate:reset && npm run migrate:up && npm run build && cross-env NODE_ENV='test' jasmine"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-pino-logger": "^4.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.8.3",
    "@types/pg": "^8.6.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.25.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "helmet": "^6.0.0",
    "pg": "^8.8.0",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.1"
  }
}
